    "def filter_dataset(dataset, words):\n",
    "    dataset = dataset.copy()\n",
    "    dataset['Question lower'] = dataset['Question'].str.lower()\n",
    "\n",
    "    return dataset[dataset['Question lower'].apply(lambda q: all(word.lower() in q for word in words))]\n",
    "\n",
    "filtered_dataset = filter_dataset(jeopardy, ['King', 'England'])\n",
    "print(filtered_dataset['Question'])"
   ]
