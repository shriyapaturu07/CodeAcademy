    "def jeopardy_quiz(dataset):\n",
    "    random_row = dataset.sample(n=1).iloc[0]\n",
    "    \n",
    "    print(\"Question:\", random_row[\"Question\"])\n",
    "    \n",
    "    user_answer = input(\"Please answer the question: \").strip().lower()\n",
    "    \n",
    "    correct_answer = str(random_row[\"Answer\"]).strip().lower()\n",
    "    \n",
    "    if user_answer == correct_answer:\n",
    "        return f\"Correct! You win {random_row['Value']}.\"\n",
    "    else:\n",
    "        return f\"Nice try! The correct answer was: {random_row['Answer']}\"\n",
    "\n",
    "\n",
    "jeopardy_quiz(jeopardy)"
   ]
